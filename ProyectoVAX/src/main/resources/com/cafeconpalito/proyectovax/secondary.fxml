<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>


<HBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cafeconpalito.proyectovax.SecondaryController">
   <children>
      <VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="390.0" prefWidth="189.0" spacing="20.0">
          <children>
              <Label text="Secondary View" />
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Switch to Primary View" />
            <Button fx:id="addGameButton" mnemonicParsing="false" onAction="#clickAddGameButton" text="Button" />
          </children>
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      </VBox>
      <ScrollPane>
         <content>
            <HBox>
               <children>
                  <TilePane fx:id="gamesTilePane" prefHeight="390.0" prefWidth="428.0" style="-fx-background-color: blue;" />
               </children>
            </HBox>
         </content>
      </ScrollPane>
   </children>
</HBox>
